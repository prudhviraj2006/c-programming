#include <stdio.h>

int main() 
{
    int i, evenSum = 0, oddSum = 0;

    for (i = 1; i <= 10; i++)
	 {
        if (i % 2 == 0)
		 {
            evenSum += i;  
        } 
		else 
		{
            oddSum += i;  
        }
    }

    printf("Sum of even numbers: %d\n", evenSum);
    printf("Sum of odd numbers: %d\n", oddSum);

    return 0;
}
