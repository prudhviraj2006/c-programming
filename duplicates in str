#include<stdio.h>
#include<string.h>
int main()
{
  int i,j,k;
  char str[100]="abacdce";
  for(i=0;i<strlen(str);i++)
  {
    for(j=i+1;str[j]!='\0';j++)
    {
      if(str[j]==str[i])
      {
        for(k=j;str[k]!='\0';k++)
        {
          str[k]=str[k+1];
        }
      }
    }
  }
  printf(" after removing all duplicates: %s",str);
  return 0;
}
